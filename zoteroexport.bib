@article{arnold2017,
  title = {Sex {{Hormones}} and {{Sex Chromosomes Cause Sex Differences}} in the {{Development}} of {{Cardiovascular Diseases}}},
  author = {Arnold, Arthur P. and Cassis, Lisa A. and Eghbali, Mansoureh and Reue, Karen and Sandberg, Kathryn},
  year = {2017},
  month = may,
  volume = {37},
  pages = {746--756},
  issn = {1524-4636},
  doi = {10.1161/ATVBAHA.116.307301},
  abstract = {This review summarizes recent evidence concerning hormonal and sex chromosome effects in obesity, atherosclerosis, aneurysms, ischemia/reperfusion injury, and hypertension. Cardiovascular diseases occur and progress differently in the 2 sexes, because biological factors differing between the sexes have sex-specific protective and harmful effects. By comparing the 2 sexes directly, and breaking down sex into its component parts, one can discover sex-biasing protective mechanisms that might be targeted in the clinic. Gonadal hormones, especially estrogens and androgens, have long been found to account for some sex differences in cardiovascular diseases, and molecular mechanisms mediating these effects have recently been elucidated. More recently, the inherent sexual inequalities in effects of sex chromosome genes have also been implicated as contributors in animal models of cardiovascular diseases, especially a deleterious effect of the second X chromosome found in females but not in males. Hormonal and sex chromosome mechanisms interact in the sex-specific control of certain diseases, sometimes by opposing the action of the other.},
  file = {/home/pietas/Zotero/storage/ZXKSQ8ME/Arnold et al. - 2017 - Sex Hormones and Sex Chromosomes Cause Sex Differe.pdf},
  journal = {Arteriosclerosis, Thrombosis, and Vascular Biology},
  keywords = {androgen effects,Animals,atherosclerosis,Cardiovascular Diseases,coronary artery disease,Disease Models; Animal,estrogen effects,Female,Genetic Predisposition to Disease,Gonadal Steroid Hormones,gonosomes,Health Status Disparities,heart failure,Humans,Male,Mice; Transgenic,obesity,Phenotype,Protective Factors,Risk Factors,Sex Chromosomes,Sex Factors},
  language = {eng},
  number = {5},
  pmcid = {PMC5437981},
  pmid = {28279969}
}

