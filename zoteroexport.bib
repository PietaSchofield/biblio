@article{hammad2008,
  title = {Determining the Predictive Value of {{Read}}/{{OXMIS}} Codes to Identify Incident Acute Myocardial Infarction in the {{General Practice Research Database}}},
  author = {Hammad, Tarek A. and McAdams, Mara A. and Feight, Andrea and Iyasu, Solomon and Dal Pan, Gerald J.},
  year = {2008},
  month = dec,
  volume = {17},
  pages = {1197--1201},
  issn = {1099-1557},
  doi = {10.1002/pds.1672},
  abstract = {PURPOSE: To determine (1) the positive predictive value (PPV) of multiple Read/OXMIS codes to identify incident acute myocardial infarction (AMI) cases in General Practice Research Database (GPRD); (2) the ability to capture the correct timing of the clinical event. METHODS: A random sample of 238 records (from 155 general practitioner (GP) practices) with AMI codes, between 1 January 1997 and 31 December 2004, was selected from GPRD. Questionnaires were sent to the GPs to verify the diagnosis and timing of code-identified incident AMI events and collect supporting information. We calculated the PPV of the AMI codes as the proportion of code-identified AMIs that the GPs confirmed as AMI cases. Two physicians from Food and Drug administration (FDA), blinded to the GP response, reviewed the supporting hospital records returned by GPs for 98 AMI cases. RESULTS: A total of 217 questionnaires were completed (91\% response rate). The PPV of the AMI codes was 93\% (201/217). Thirty one (15\%) cases had a different event date than the one recorded in the electronic medical records (EMR); 28 (90\%) of the dates were within 15 days. One GP indicated that a patient had a previous AMI, 110 days before the codes that captured the AMI diagnosis. A total of 159 (79\%) AMI cases were hospitalized; hospital records were provided for 98 patients. Physician review of the hospital records found that 96\% of these records had enough information for classification, but not independent diagnosis, of AMI. CONCLUSIONS: Information in GPRD is sufficient to identify incident AMI cases and determine the event date with reasonable accuracy.},
  journal = {Pharmacoepidemiology and Drug Safety},
  keywords = {Databases; Factual,Family Practice,Humans,Incidence,International Classification of Diseases,Myocardial Infarction,Predictive Value of Tests,Surveys and Questionnaires},
  language = {eng},
  number = {12},
  pmid = {18985705}
}

