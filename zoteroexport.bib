  isbn = {978-1-4419-7802-8},
  keywords = {Cell Suppression,Data Utility,Internal Cell,Marginal Cell,Output Pattern},
  language = {en},
  series = {Statistics for {{Social}} and {{Behavioral Sciences}}}
}

