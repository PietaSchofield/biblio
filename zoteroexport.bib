@article{guss2017,
  title = {Disordered {{Weight Management Behaviors}}, {{Nonprescription Steroid Use}}, and {{Weight Perception}} in {{Transgender Youth}}},
  author = {Guss, Carly E. and Williams, David N. and Reisner, Sari L. and Austin, S. Bryn and {Katz-Wise}, Sabra L.},
  year = {2017},
  month = jan,
  volume = {60},
  pages = {17--22},
  issn = {1054-139X},
  doi = {10.1016/j.jadohealth.2016.08.027},
  abstract = {Purpose Disordered weight management behaviors are prevalent among youth; recent case reports suggested that these behaviors might also be common in transgender youth. We studied associations of gender identity with disordered weight management behaviors, nonprescription steroid use, and weight perception among transgender and cisgender (nontransgender) high-school students in Massachusetts. Methods Data were analyzed from the 2013 Massachusetts Youth Health Survey, an anonymous survey in a random sample of Massachusetts public high schools. Respondents were divided into three groups: transgender (n~= 67), cisgender male (n~= 1,117), and cisgender female (n~= 1,289). Fisher's exact tests and multivariable logistic regression models were used to examine unhealthy weight management behaviors in the past 30~days: fasting {$>$}24~hours, vomiting, diet pill use, and laxative use; nonprescription steroid use; and self-perceived weight status. Analyses controlled for age, race/ethnicity, and body mass index. Results Compared with cisgender males, transgender adolescents had higher odds of fasting {$>$}24~hours (adjusted odds ratio [AOR]~= 2.9, confidence interval [CI]~= 1.1\textendash 7.8), using diet pills (AOR~= 8.9, 95\% CI~= 2.3\textendash 35.2) and taking laxatives (AOR~= 7.2, 95\% CI~= 1.4\textendash 38.4). Transgender youth had higher odds of lifetime use of steroids without a prescription than male cisgender respondents (AOR~= 26.6, 95\% CI~= 3.5\textendash 200.1). Compared with cisgender females, transgender respondents had higher odds of perceiving themselves as healthy weight/underweight when they were overweight/obese (AOR~= 2.4, 95\% CI~= 1.5\textendash 4.1). Conclusions Transgender youth disproportionately self-reported unsafe weight management behaviors and nonprescription steroid use compared with cisgender youth. Clinicians should be aware of this increased risk among transgender youth. Research is needed to further understand these disparities and to inform future interventions.},
  journal = {Journal of Adolescent Health},
  keywords = {Adolescent,Eating disorder,Steroid,Transgender},
  language = {en},
  number = {1}
}

@article{guy2010,
