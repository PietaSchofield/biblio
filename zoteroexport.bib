@article{han2009,
  title = {Substance Use Disorder among Older Adults in the {{United States}} in 2020},
  author = {Han, Beth and Gfroerer, Joseph C. and Colliver, James D. and Penne, Michael A.},
  year = {2009},
  volume = {104},
